# APP
FUNCTION_NAME=typology-processor
NODE_ENV=dev
PORT=3000
QUOTING=false
APM_ACTIVE=false
MAX_CPU=1

# REDIS
REDIS_DB=0
REDIS_AUTH=
REDIS_SERVERS=[{"host":"redis", "port":6379}]
REDIS_IS_CLUSTER=false

# NATS
SERVER_URL=nats:4222
STARTUP_TYPE=nats
CONSUMER_STREAM=RuleResponse901
PRODUCER_STREAM=TADP
ACK_POLICY=Explicit
PRODUCER_STORAGE=File
PRODUCER_RETENTION_POLICY=Workqueue

# ARANGO
DATABASE_URL=tcp://arango:8529
DATABASE_USER=root
DATABASE_PASSWORD=
DATABASE_NAME=Configuration
DATABASE_NETWORKMAP=networkmap
COLLECTION_NAME=typologyExpression
CACHE_TTL=300